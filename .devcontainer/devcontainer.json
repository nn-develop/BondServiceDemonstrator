{
	"name": "Bond Service Demonstrator Dev Container",
	"dockerFile": "../Dockerfile",
	"context": "..",
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python",
				"ms-python.vscode-pylance",
				"ms-python.black-formatter"
			],
			"settings": {
				"python.formatting.provider": "black",
				"python.languageServer": "Pylance",
				"editor.formatOnSave": true
			}
		}
	},
	"remoteUser": "devuser"
}